package d;
abstract class Employee{
	private int id;
	private String name;
	private double salary;
	public Employee(int id, String name) {
		this.id = id;
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public String getName() {
		return name;
	}

	abstract public double getSalary() ;
}